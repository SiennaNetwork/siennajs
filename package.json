    "@hackbg/konzola": "^2",
    "@types/js-yaml": "^4.0.5",
    "crypto-js": "^4.1.1",
    "js-yaml": "^4.1.0",
    "merkletreejs": "^0.2.32"
  },
  "devDependencies": {
    "@hackbg/izomorf": "^4.2.3",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^17.0.45",
    "concurrently": "7.2.2",
    "typescript": "^4.7.4"
